enum UserType {
  ADMIN,
  USER
}

type User {
  id: ID! @id
  email: String! @unique
  password: String!
  name: String!
  surname: String!
  address: String
  refreshTime: Int @default(value: 5)
  backgroundImage: String
  sidebarDisabled: Boolean
  googleToken: String
  intraToken: String
  spotifyToken: String
  timezone: String
  phone: String! @unique
  type: UserType @default(value: USER)
}
